.nav
	flex 1
	margin 0 25px
	text-align center
	position relative
	li
		display inline-block
		margin-right 56px
		&:last-child
			margin-right 0
			position absolute
			right 200px
			
		&.is-active
			a
				opacity .75
	
	a
		text-transform uppercase
		color $text-color
		font 18px $SSemiBold
		&:hover
			opacity .75